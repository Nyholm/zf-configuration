    "description": "ZF2 Module providing REST resource for manipulating configuration",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rest",
        "config"
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com" },
        { "type": "composer", "url": "http://packages-zfcampus.rhcloud.com" }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zfcampus/zf-content-negotiation": "dev-master",
        "zendframework/zendframework": "dev-develop"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
